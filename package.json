    "eslint": "node_modules/.bin/eslint --ext .jsx --ext .js .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "build": "NODE_ENV=production webpack -p",
    "dev": "webpack-dev-server --env development",
    "dev-server": "docker-compose -f docker-compose-server.yml up",
